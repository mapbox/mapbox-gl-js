    {
      "id": "literal",
      "type": "symbol",
      "source": "mapbox",
      "layout": {
        "text-field": ["let",
            "a", ["to-string", ["get", "a"]],
            "b", ["to-string", ["get", "b"]],
            "op", ["get", "operator"],
            "collation", ["collator", {
                "case-sensitive": ["get", "case-sensitive"],
                "diacritic-sensitive": ["get", "diacritic-sensitive"],
                "locale": "en" }
            ],
            ["concat",
                ["var", "a"], " ",
                ["var", "op"], " ",
                ["var", "b"], "\n",
                "Case: ", ["to-string", ["get", "case-sensitive"]], "\n",
                "Diacritics: ", ["to-string", ["get", "diacritic-sensitive"]], "\n",
                ["to-string", ["match",
                    ["var", "op"],
                    "==", ["==", ["var", "a"], ["var", "b"], ["var", "collation"]],
                    "!=", ["!=", ["var", "a"], ["var", "b"], ["var", "collation"]],
                    "<",  ["<",  ["var", "a"], ["var", "b"], ["var", "collation"]],
                    "<=", ["<=", ["var", "a"], ["var", "b"], ["var", "collation"]],
                    ">",  [">",  ["var", "a"], ["var", "b"], ["var", "collation"]],
                    ">=", [">=", ["var", "a"], ["var", "b"], ["var", "collation"]],
                    false
                ]]
            ]
        ],
        "text-font": [
          "Open Sans Semibold",
          "Arial Unicode MS Bold"
      ],
        "text-size": 10,
