          {
            "type": "Feature",
            "geometry": {
              "type": "Point",
              "coordinates": [
                0,
                0
              ]
            }
          }
        ]
      }
    }
  },
  "layers": [
    {
      "id": "layer-with-dot",
      "type": "symbol",
      "source": "dot-point",
      "layout": {
        "icon-image": "dot"
      }
    }
  ]
}
