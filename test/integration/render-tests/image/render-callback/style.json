{
  "version": 8,
  "metadata": {
    "test": {
      "width": 200,
      "height": 200,
      "operations": [
        [
          "addImage",
          "dot",
          "./image/dot.js"
        ],
        [
          "wait"
        ]
      ]
    }
  },
  "sources": {
    "dot-point": {
      "type": "geojson",
      "data": {
        "type": "FeatureCollection",
