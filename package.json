{
  "name": "mapbox-gl",
  "description": "A WebGL interactive maps library",
  "version": "0.0.13",
  "main": "js/mapbox-gl.js",
  "license": "BSD",
  "dependencies": {
    "color": "^0.6.0",
    "geojson-rewind": "~0.1.0",
    "mapbox-gl-style-spec": "0.0.1",
    "pbf": "0.0.1",
    "queue-async": "^1.0.7",
    "rbush": "~1.3.2",
    "unitbezier": "0.0.0"
  },
  "devDependencies": {
    "benchmark": "~1.0.0",
    "browserify": "~4.1.8",
    "faucet": "0.0.1",
    "glify": "^0.3.0",
    "hintify": "^0.1.0",
    "istanbul": "^0.2.10",
    "jshint": "^2.5.1",
    "spritesmith": "~0.19.2",
    "tape": "~2.13.2",
    "uglify-js": "~2.4.13",
    "watchify": "~0.10.2",
    "brfs": "~1.1.1",
    "mocha": "~1.20.1",
    "zuul": "~1.7.0",
    "envify": "~2.0.0"
  },
  "browserify": {
    "transform": [
      "envify",
      "hintify",
      "glify",
      "brfs"
    ]
  },
  "scripts": {
    "test": "tape test/*.js | faucet",
    "test-remote": "zuul -- test/*.js",
    "watch": "watchify -v -d js/mapbox-gl.js -o dist/mapbox-gl-dev.js",
    "build": "browserify -d js/mapbox-gl.js > dist/mapbox-gl-dev.js",
    "production": "browserify -i js/render/draw{vertices,debug}.js -d js/mapbox-gl.js | uglifyjs -c drop_console=true > dist/mapbox-gl.js",
    "prepublish": "mkdir -p dist && npm run build && npm run production",
    "cov": "istanbul cover tape test/*.js -x js/lib/glmatrix.js"
  }
}
