      "line-width": "@service_casing_width"
    },
    "type": "line"
   }, {
    "id": "tunnel_street_casing",
    "source": "mapbox",
    "source-layer": "tunnel",
    "filter": { "class": ["street", "street_limited"] },
    "render": {
      "line-join": "round"
    },
    "style": {
      "line-color": "@street_casing",
      "line-dasharray": [7, 2],
      "line-width": "@street_casing_width",
      "line-opacity": "@street_casing_opacity"
    },
    "type": "line"
  }, {
    "id": "tunnel_main_casing",
    "source": "mapbox",
    "source-layer": "tunnel",
    "filter": { "class": "main" },
    "render": {
      "line-cap": "round",
      "line-join": "round"
    },
