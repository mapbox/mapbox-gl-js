      "id": "admin_level_3",
      "source": "mapbox",
      "source-layer": "admin",
      "filter": {
        "admin_level": {">=": 3},
        "maritime": 0
      },
      "render": {
      },
      "style": {
        "line-color": "@admin",
        "line-opacity": "@admin-opacity",
        "line-dasharray": [10, 3],
        "line-width": "@admin_level_3_width"
      },
      "type": "line"
    }, {
      "id": "admin_level_2",
      "source": "mapbox",
      "source-layer": "admin",
      "filter": {
