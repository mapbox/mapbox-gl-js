    },
    "style": {
      "line-color": "@street_casing",
      "line-width": "@service_casing_width"
    },
    "type": "line"
  }, {
    "id": "road_street_casing",
    "source": "mapbox",
    "source-layer": "road",
    "filter": { "class": ["street", "street_limited"], "$type": "LineString" },
    "render": {
      "line-cap": "round",
      "line-join": "round"
    },
    "style": {
      "line-color": "@street_casing",
      "line-width": "@street_casing_width",
